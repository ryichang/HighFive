<div class="container">
	<div class="row">
		<div id="profile-info" class="col-sm-offset-1 col-sm-3 affix">
			<%= image_tag @user.avatar.url %>
			<h1><%= @user[:first_name] %> <%= @user[:last_name] %></h1>
			
			<p></p> <!-- padding -->
<!-- 			<p>Bio</p> -->
    		<p><strong>Join Date: </strong><small><%= time_ago_in_words(@user.created_at)%> ago</small></p>
    		<%=link_to "Edit profile", user_path, class:"btn btn-primary" %> 
    		<p></p> <!-- padding -->
		</div>
		<div id="profile-data" class="col-sm-offset-5 col-sm-6">
			<h2>Compliments</h2>

		</div>
	</div>
</div>